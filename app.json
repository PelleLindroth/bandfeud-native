{
  "expo": {
    "name": "bandfeud",
    "slug": "bandfeud",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/play-icon.png",
    "splash": {
      "image": "./src/assets/bandfeud-logo-square.png",
      "resizeMode": "contain",
      "backgroundColor": "#333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/bandfeud-logo-square.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.ico"
    }
  }
}
